@model Modelo.Cadastros.Project

<div class="card-body">
    <div class="rom">
        <div class="col-8">
            <div class="form-group">
                <label asp-for="ProjectId" class="control-label"></label>
                <br />
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fa fa-key" aria-hidden="true"></i>
                        </span>                        
                    </div>
                    <input asp-for="ProjectId" class="form-control" disabled="disabled"/>
                </div>
                <label asp-for="Name" class="control-label"></label>
                <br />
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fa fa-key" aria-hidden="true"></i>
                        </span>                        
                    </div>
                    <input asp-for="Name" class="form-control"  disabled="disabled"/>
                </div>                
            </div>
        </div>
        <div class="col-4">
            @{await Html.RenderPartialAsync("~/Views/Project/_WithActivities.cshtml", Model.Activities.ToList());}
        </div>
    </div>
</div>
